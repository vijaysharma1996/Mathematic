//You are given a number N. You have to find the minimum number of operations required to reach N from 0. You have two valid operations available:

//Double the number
//Add one to the number

#include <bits/stdc++.h>
using namespace std;
int main()
{   int num,count=0;
    cin>>num;
    while(num!=0)
    {
        if(num%2==0)
        num=num/2;
        else
        num=num-1;
        count++;
    }
    cout<<"count:"<<count;

    return 0;
}
